  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/k-ring-bot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/k-ring-bot/issues"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "mathjs": "^12.4.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
